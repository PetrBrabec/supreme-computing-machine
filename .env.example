# Domain Configuration
APP_DOMAIN=example.com
CADDY_ACME_EMAIL=admin@example.com

# Repository Configuration
SETUP_REPOSITORY=https://github.com/username/supreme-computing-machine.git

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeThisPassword
POSTGRES_DB=n8n
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=changeThisPasswordToo

# Appwrite Configuration
APP_ENV=production
APP_OPENSSL_KEY=changeThisKey

# Baserow Configuration
BASEROW_SECRET_KEY=changeThisSecretKey
BASEROW_DB_PASSWORD=changeThisBaserowPassword

# Qdrant Configuration
QDRANT_API_KEY=changeThisQdrantKey

# Minio Configuration
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=changeThisMinioPassword

# Redis Configuration
REDIS_PASSWORD=changeThisRedisPassword

# Keycloak Configuration
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=changeThisKeycloakPassword
KC_DB=postgres
KC_DB_URL=jdbc:postgresql://postgres:5432/keycloak
KC_DB_USERNAME=keycloak
KC_DB_PASSWORD=changeThisKeycloakDbPassword
KC_HOSTNAME=your-domain.com
KC_PROXY=edge

# Backup Configuration
RESTIC_PASSWORD=your-restic-password
BACKUP_CRON="0 2 * * *"
BACKUP_VOLUME_PATH=/dev/disk/by-id/scsi-0HC_Volume_101626985
BACKUP_MOUNT_POINT=/mnt/volume-scm-backup
RESTIC_REPOSITORY=/mnt/volume-scm-backup/restic-repo
TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_CHAT_ID=your_chat_id
